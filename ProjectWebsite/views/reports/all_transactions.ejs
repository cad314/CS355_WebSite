<% include ../partials/header %>

<section class="report">
    <div class="display_table">
        <table class="display_table">
            <thead>
            <th width="20%">Source</th><th width="15%">Date</th><th width="15%">Value</th><th width="50%">Description</th>
            </thead>
        </table>
        <table class="table table-striped table-hover table-bordered">
            <tbody>
                <%for(var i = 0; i < rs.length; i++){%>
                <tr>
                    <td width="20%"><a href="/report/transactions/one/?transaction_id=<%=rs[i].transaction_id%>"><%=rs[i].source_name%></a></td>
                    <td width="15%"><a href="/report/transactions/one/?transaction_id=<%=rs[i].transaction_id%>"><%=rs[i].trans_date%></a></td>
                    <td width="15%" style="text-align: right"><a href="/report/transactions/one/?transaction_id=<%=rs[i].transaction_id%>"><%=rs[i].trans_value%></a></td>
                    <td width="50%"><a href="/report/transactions/one/?transaction_id=<%=rs[i].transaction_id%>"><%=rs[i].description%></a></td>
                </tr>
                <%}%>
            </tbody>
        </table>
    </div>
</section>

<% include ../partials/footer %>