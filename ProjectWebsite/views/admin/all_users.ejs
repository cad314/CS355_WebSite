<% include ../partials/header %>

<section class="admin">
    <div class="display_table">
        <table class="display_table">
            <thead>
                <th width="25%">First</th><th width="25%">Last</th><th width="35%">Email</th><th width="15%">Access Level</th>
            </thead>
        </table>
        <table class="table table-striped table-hover table-bordered">
            <tbody>
                <%for(var i = 0; i < rs.length; i++){%>
                <tr>
                    <td width="25%"><a href="/admin/one/?account_id=<%=rs[i].account_id%>"><%=rs[i].first_name%></a></td>
                    <td width="25%"><a href="/admin/one/?account_id=<%=rs[i].account_id%>"><%=rs[i].last_name%></a></td>
                    <td width="35%"><a href="/admin/one/?account_id=<%=rs[i].account_id%>"><%=rs[i].email%></a></td>
                    <td width="15%"><a href="/admin/one/?account_id=<%=rs[i].account_id%>"><%=rs[i].access_lvl%></a></td>
                </tr>
                <%}%>
            </tbody>
        </table>
    </div>
</section>

<% include ../partials/footer %>